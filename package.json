{
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  },
  "scripts": {
    "tw": "npx @tailwindcss/cli -i ./assets/tailwind.css -o ./assets/styles.css",
    "tww": "npx @tailwindcss/cli -i ./assets/tailwind.css -o ./assets/styles.css --watch",
    "dev": "concurrently -k \"npm:tww\" \"cargo tauri dev\"",
    "tb": "cargo tauri build"
  }
}
